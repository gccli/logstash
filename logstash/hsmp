input {
  syslog {
    port => 5530
    add_field => {
      "vendor" => "hsmp"
    }
  }
}

filter {
   json {
     source => "message"
     #remove_field => ["message"]
   }
}

output {
  stdout { codec => rubydebug }

  redis {
        data_type => "list"
        host => ["redis"]
        key => "logstash"
        codec => json
  }
}
